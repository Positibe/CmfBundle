{% extends '@PositibeCmf/layout.html.twig' %}

{% block title %}Categoría{% endblock %}
{% block subtitle %}Crear nueva categoría{% endblock %}
{% set menu_active = 'content' %}
{% set content_menu = 'category' %}

{% block breadcrumb_list %}
    <li>
        <i class="fa fa-angle-right"></i>
        <a href="{{ path('positibe_category_index') }}">Categoría</a>
    </li>
    <li>
        <i class="fa fa-angle-right"></i>
        Nueva categoría
    </li>
{% endblock %}

{% block content %}
    {{ form_start(form,{'attr': {'class': 'form-horizontal inbox-compose' }}) }}
    {{ include('@PositibeCmf/Category/_form.html.twig') }}
    {{ form_end(form) }}
{% endblock %}

{% block stylesheets %}
    {{ include('@PositibeCmf/_datepicker_resources.html.twig') }}
{% endblock %}